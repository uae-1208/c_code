25.#include<stdio.h>
 int main(void)
 {
	float speed,size,time;
	printf("The size(MB) of the file is:");
	scanf("%f",&size);
	printf("The downloading speed(Mb/s) is:");
	scanf("%f",&speed);
	printf("At %.2f megabits per second, a file of %.1f megabytes downloads in %.2f seconds",
	speed,size,size*8/speed);
  }
  
  
26.#include<stdio.h>
 int main(void)
 {
	char first_name[20],last_name[20];
	int a,b;
	printf("Please enter your first name:");
	scanf("%s",first_name);
	printf("Please enter your last name:");
	scanf("%s",last_name);
	a=strlen(first_name);
	b=strlen(last_name);
	printf("%s %s\n",first_name,last_name);
	printf("%*d %*d\n",a,a,b,b);
	printf("%s %s\n",first_name,last_name);
	printf("%-*d %-*d\n",a,a,b,b);
 }




