8、#include<stdio.h>
int main()
{
    int price=0,money=0;
    printf("请输入应付金额：");
    scanf("%d",&price);
    printf("请输入实付金额：");
	scanf("%d",&money);
	int change=money-price;
	if (change<0)
	{
		printf("实付金额不足，请再支付%d元",-change);
	 } 
	 if(change>=0)
	 {
	 printf("找零%d元",change);
	 } 
	return 0;
}


 
9、#include<stdio.h>
int main()
{
    int score100; 
    printf("请输入百分制成绩：");
	scanf("%d",&score100);
	if(90<=score100)
		printf("您的五分制成绩为：A");
	else if(80<=score100&score100<90)
		printf("您的五分制成绩为：B");
	else if(70<=score100)
		printf("您的五分制成绩为：C");
	else if(60<=score100)
		printf("您的五分制成绩为：D");
	else
		printf("您的五分制成绩为：E");
	return 0;
}


10、#include<stdio.h>
int main()
{
    int score100; 
    printf("请输入百分制成绩：");
	scanf("%d",&score100);
	int a=score100/10;
	switch(a)
	{
		case 10:
		case 9:
			printf("您的五分制成绩为：A"); 
			break;
		case 8:
			printf("您的五分制成绩为：B");
			break;
		case 7:
			printf("您的五分制成绩为：C");
			break;
		case 6:
			printf("您的五分制成绩为：D"); 
			break;
		default:
			printf("您的五分制成绩为：E");
	}
	return 0;
}



11、#include<stdio.h>
int main()
{
    int a,n=1;
	printf("请任意输入一个正整数：");
	scanf("%d",&a);
	while(a/10>0)
	{
		a/=10;
		n++;
	 } 
	 printf("该正整数的位数为%d",n);
	return 0;
}



12、#include<stdio.h>
#include<stdlib.h>
#include<time.h>
int main()
{
    srand(time(0));
    int a=rand()%100+1;
    int c=0,b=0;
    printf("我已经想好了一个数字，快来猜猜吧！\n");
    printf("和我说说你猜的数字：\n");
    do
    {
    	scanf("%d",&b);
    	c++;
    	if(b>a){
    		printf("你猜的数字太大了，再猜猜看！\n");
		}else if(b<a){
			printf("你猜的数字太小了，再猜猜看！\n");
		}
    	
	}while(b!=a);
	printf("太棒了!答案就是%d。你只用了%d次就猜对了！",a,c);
	return 0;
}



13、#include <stdio.h>
int main()
{
	int x;
	printf("请输入一个整数（在0到30之间）:");
	scanf("%d",&x); 
	if(1<=x&x<=10)
	printf("......");
	else if(11<=x&x<=20)
	printf("《》");
	else if(21<=x&x<=30)
	printf("w..w");
	
	return 0;
 } 
 
 

14、#include <stdio.h>
int main()
{
	int x, y;
	printf("输入x= ");
	scanf_s("%d", &x);
	printf("输入y= ");
	scanf_s("%d", &y);
	if (x > y)
		printf("x>y");
	else if (x < y)
		printf("x<y");
	else
		printf("x=y");
	return 0;
}



15、#include <stdio.h>
int main()
{
	int x=0, y=0;
	printf("输入x= ");
	scanf_s("%d", &x);
	
	if (x < 0)
		y = 2 * x - 1; 
	else if (x == 0)
		y = 0;
	else if (x>0)
		y = 3 * x;
	printf("y= %d", y);
	return 0;
}	



16、#include<stdio.h>
int main()
{	
	int number,n,count=0;
	printf("请输入待检测数字:");
	scanf("%d",&number);
	for(n=2;n<number;n++)
	{
		if(number % n==0)	
			count++;
	}
	if(count==0)
		printf( "该数字为素数");
	else
		printf( "该数字不为素数");
	return 0;
}



17. #include <stdio.h>
void jolly(void);     
void deny(void);     //引入两个其他函数

int main(void)
{
	jolly();
	jolly();
	jolly();
	deny();
return 0;
 }
 
 void jolly (void)    //定义两个函数 
 {
 	printf("For he's a jolly good fellow!\n");
 }
 void deny(void) 
 {
 	printf("Which nobody can deny!");
 }




