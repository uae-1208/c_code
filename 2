27.#include<stdio.h>
 int main(void)
{
	int week,day,time;
	printf("Enter days:");
	scanf("%d",&time);
	while(time>0)
	{
		week = time / 7;
		day = time % 7;
		printf("%d days are %d weeks,%d days\n",time,week,day);
		printf("Enter another time:\n");
		scanf("%d",&time);
	}
}




28.#include<stdio.h>
 int main(void)
{
  int day;
	int money=0;
	int count_day=1;
	printf("Please enter the number of day:");
	scanf("%d",&day);
	while(count_day<day+1)
	{
		money=money+count_day*count_day;	
		count_day++;
	}
	printf("The money you earn is $%d.",money);
 }
 
 
 
 
 
29. #include <stdio.h>
#include <string.h>
int main(void)
{
	int i;
	char ch[26];
	ch[0]='a';
	for(i=0;i<26;i++)
	{
		ch[i]='a'+i;
		printf("ch[%d] is '%c'\n",i,ch[i]);
	}
 }
 
 
 
 
 
 
 
30.#include<stdio.h>
int main(void)
{
	int i,n;
	char ch;
	for(i=1;i<7;i++)
	{
		for(n=1,ch='F';n<=i;n++)
			printf("%c",ch-n+1);
		printf("\n");
	}
}




31.#include<stdio.h>
int main(void)
{
	char ch;
	int i,n,m;
	m=1;
	for(i=1;i<7;i++)
	{
		ch='A'+m-1;
		for(n=1;n<=i;n++)
		{	
			printf("%c",ch,n);
			ch=ch+1;
		} 
		printf("\n");
		m=m+i;
	}
 }
 
 
 
 
32.#include<stdio.h>
int main(void)
{
	int uplim,dolim;
	int i;
	printf("Please enter the uplimit and the downlimit:");
	scanf("%d%d",&uplim,&dolim);
	for(i=dolim;i<=uplim;i++)
		printf("%5d%10d%10d\n",i,i*i,i*i*i);
}






33.#include<stdio.h>
int main(void)
{
  char word[20];
	char inverse_word[20];
	int length,i;
	printf("Please enter a word:");
	scanf("%s",word);
	length=strlen(word);
	for(i=0;i<length;i++)
		inverse_word[length-i-1]=word[i];
	printf("%s",inverse_word);
}




34.#include<stdio.h>
int main(void)
{
  int lower,upper,sum,i;
	printf("Enter lower and upper integer limits:");
	scanf("%d %d",&lower,&upper);
	sum=0;
	while(lower<upper)
	{
		for(i=lower;lower<=upper;i++)
			sum=sum+i*i;
		printf("The sum of the squares from %d to %d is %d\n",lower*lower,upper*upper,sum);
		printf("Enter next set of limits:");
		scanf("%d%d",&lower,&upper);
	}
	printf("Done!");
}



35.#include<stdio.h>
int main(void)
{
  int integer[8];
	int rev_in[8];
	int i;
	printf("Enter 8 integers:");
	for(i=0;i<8;i++)
		scanf("%d",&integer[i]);
	for(i=0;i<8;i++)
	{
		rev_in[i]=integer[8-i-1];
		printf("%5d",rev_in[i]);
	}
}  
 
 
 
 
 
36.#include<stdio.h>
int main(void)
{
  int times,i;
	float num1,num2;
	do
	{
		num1=num2=0.0;
		printf("请输入运行次数（输入0退出）：");
		scanf("%d",&times);
		for(i=1;i<=times;i++)
			num1=num1+1.0/i;
		printf("num1=%f\n",num1);
	}while(times!=0);
}






37.#include<stdio.h>
int main(void)
{
  int integer[8];
	int i,num=2;
	for(i=0;i<8;i++)
	{
		integer[i]=num;
		num*=2;
	}
	i=0;
	do
	{
		printf("integer[%d] is %d\n",i,integer[i]);
		i+=1;
	}while(i<8);
}





38.#include<stdio.h>
int main(void)
{
  int num[8],sum[8];
	int i,sumk=0;
	for(i=0;i<8;i++)
	{
		printf("Enter a number for num[%d]:",i);
		scanf("%d",&num[i]);
		sumk=sumk+num[i];
		sum[i]=sumk;
	}
	for(i=0;i<8;i++)	
		printf("%5d",num[i]);
	printf("\n");
	for(i=0;i<8;i++)
		printf("%5d",sum[i]);
}






39.#include<stdio.h>
int main(void)
{
  float mon1=100.0;
	float mon2=100.0;
	int i=2;
  mon1+=100*0.1;
	mon2*=1+0.05;	
	while(mon1>mon2)
	{
		mon1+=100*0.1;
		mon2*=1+0.05;
		i++;
	}
	printf("In the %dth year,the money of de is %f > the money of da is %f",i,mon2,mon1);
}



40.#include<stdio.h>
int main(void)
{
  float mon=100.0;
	int year=2;
	while(mon>0)
	{
		mon=mon*(1+0.08)-10;
		printf("%f  ",mon);
		printf("%d\n",year);
		year++;
	}
	printf("%d",year);
  
  
  
40.#include<stdio.h>
int main(void)
{
	const int dunbar=150;
	int fri_num=5,week_num;
	printf("The number of week   The number of friend\n");
	for(week_num=1;fri_num<dunbar;week_num++)
	{
		fri_num=(fri_num-week_num)*2;
		printf("%-21d%d\n",week_num,fri_num);
	}
	printf("Done!");
 }






41. #include <stdio.h>
#include <string.h>
int main()
{
	char letter;
 	int row;         //控制行 
	int i;           //控制前半部分空格 
	int n;           //控制升序打印 
	int m;           //控制降序打印 
	int a;           //控制行数 
	printf("Enter a center letter(A to Z):");
	scanf("%c",&letter);
	a=(int)(letter-'A')+1;
 	for(row=1;row<=a;row++)
 	{
 		for(i=a-row+1;i>0;i--)
 			printf(" ");
		 for(n=1;n<=row;n++)
		 	printf("%c",'A'+n-1);
		 for(m=row-1;m>0;m--)
		 	printf("%c",'A'+m-1);
		printf("\n");
	 }
Return 0;
}




42.#include<stdio.h>
	#include<string.h>
	Int main()
{
	char ch[254];
	 char m;
	 int i=0,length;
	 printf("Enter anything:");
	 scanf("%c",&m);
	 while(m!='\n')
	 {
	 	ch[i]=m;
	 	i++;
	 	scanf("%c",&m);
	 }
	 length=strlen(ch);
	 for(i=length;i>=0;i--)
	 printf("%c",ch[i]);
	 return 0;
}





43. #include <stdio.h>
#include <ctype.h>
#define stop '@'
int main()
{	
	int line=0;             //计行数 
	int word=0;			    //计单词数 
	int chars=0;			//计非空字符数 
	char c;					//读入字符 
	char prec=' ';			//存储上一个字符
	printf("Enter text to be analyzed(@ to end):");
	while((c=getchar())!=stop)
	{
		chars++;
		if(c=='\n')
			line++;
		if(isspace(prec) && !isspace(c))
			word++;
		prec=c;			
	}
	if(prec!='\n')
		line++;
	printf("charasters=%d,words=%d,lines=%d",chars,word,line);
	return 0;
}
