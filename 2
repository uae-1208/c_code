2.
#include<stdio.h>
int main()
{
int hour1,hour2,minute1,minute2;
	printf("请输入两个时间：\n");
	scanf ("%d %d %d %d",&hour1,&minute1,&hour2,&minute2);
	int hour=(hour1*60+minute1-hour2*60-minute2)/60;
	int minute=(hour1*60+minute1-hour2*60-minute2)%60;
	if(minute<0)
	{
		minute=60+minute;
		hour--;
	}
	printf("时间差为%d时%d分",hour,minute);
	return 0;
}

 
