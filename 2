	return 0;
}

 
4.
#include<stdio.h>
int main()
{
	int a,b,c;
	printf("输入现在时间（三或四位数字）：\n");
	scanf("%d",&a);
	printf("输入流失时间（分钟）：\n");
	scanf("%d",&b);
	c=a/100*60+a%100+b;
	int d=c/60*100+c%60;
	printf("现在时间为：%d\n",d);
	return 0;
}
 
 
5.
#include<stdio.h>
int main()
{
	int a;
	printf("请输入一个三位正整数（如123）：\n");
	scanf("%d",&a);
	int b=a/100,c=(a-100*b)/10,d=a-100*b-10*c,f=100*d+10*c+b;
	printf("逆序的三位数为：%d",f);
	return 0;
}
 
 
 
6、
#include<stdio.h>
int main()
{
	int height;
	printf("请输入身高（cm）：\n");
	scanf("%d",&height);
	int foot=height/30.48;
	int inch=(height/30.48-foot)*12;
	printf("身高为%d英尺%d英寸",foot,inch);
	return 0;
}


